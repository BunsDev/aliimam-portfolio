{
  "name": "ai-tailwinds",
  "version": "0.0.1",
  "description": "The Tailwind CSS configuration for ai projects",
  "license": "MIT",
  "main": "tailwind.config.ts",
  "files": [
    "tailwind.config.ts"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "ai-eslints": "workspace:*",
    "ai-tsconfigs": "workspace:*",
    "tailwindcss": "^3.4.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,json,md,mdx}": "prettier --cache --write"
  }
}